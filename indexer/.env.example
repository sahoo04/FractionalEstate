# Blockchain Configuration
RPC_URL=https://sepolia-rollup.arbitrum.io/rpc
CHAIN_ID=421614

# Start Block Configuration
# - Set to 0 (default): Start from current block (only index new events going forward)
# - Set to contract deployment block: Index all historical events from that block
# - Example: START_BLOCK=12345678 (use the block number when contracts were deployed)
START_BLOCK=0

# Contract Addresses (from deployments.json)
# These will be auto-filled by setup script, or set manually:
PROPERTY_SHARE_ADDRESS=0x45e5923EB76Ee2b90B89d061436E3483f9DcC883
REVENUE_SPLITTER_ADDRESS=0xaB6873d0F1E5bD08e10d46F21aEB6608963B7bAC
MARKETPLACE_ADDRESS=0x48336fEe63694a2c874209F7C88c17C8c286A0cb
USER_REGISTRY_ADDRESS=0x8732ea38Bb0cb8daC815A1CE4dafBE206a40D995
ZK_REGISTRY_ADDRESS=0x4c01b3A4724D85Bf5d4913D2bF40CEA27b59a7d7
IDENTITY_SBT_ADDRESS=0x7390325CC470bd9831160e6F3803dc5755e7a922

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Indexer Configuration
POLL_INTERVAL=5000
BATCH_SIZE=1000
CONFIRMATIONS_REQUIRED=3
MAX_REORG_DEPTH=100
ENABLE_REORG_PROTECTION=true

# Logging
LOG_LEVEL=info
